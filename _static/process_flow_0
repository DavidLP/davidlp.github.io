digraph G {
	node [color=grey shape=square]
	compound=true label="Production processes" labelloc=t
	"FE wafer production" -> "FE wafer probing"
	"Sensor wafer production" -> "Sensor wafer post processing"
	"FE wafer probing" -> "FE wafer post processing"
	"FE wafer post processing" -> "FE selection"
	"FE selection" -> "Flip chip"
	"Sensor wafer post processing" -> "Sensor 3D selection"
	"Sensor wafer post processing" -> "Sensor planar selection"
	"Sensor 3D selection" -> "Flip chip"
	"Sensor planar selection" -> "Flip chip"
	"Flip chip" -> Installation
}
